    <% elsif designer_signed_in? %>

    <ul class="list-inline flex-inline">
      <li>
        <div class="dropdown">
          <div class="avatar dropdown-toggle" style="background-image:url('<%= image_path retailer_avatar_url(current_designer) %>');" data-toggle="dropdown"></div>

          <ul class="dropdown-menu dropdown-menu-left navbar-mox-moda-dropdown-menu">
            <li>
              <%= link_to current_designer do %>
              <i class="fa fa-user"></i> <%= t(".profile", default: "Profile") %>
              <% end %>
            </li>
            <li>
              <%= link_to "#" do %>
              <i class="fa fa-home"></i>  <%= t(".profile", default: "Account") %>
              <% end %>
            </li>
            <li>
              <%= link_to destroy_designer_session_path, method: :delete do %>
              <i class="fa fa-sign-out"></i>  <%= t(".sign_out", default: "Log out") %>
              <% end %>
