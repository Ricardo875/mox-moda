        <% end %>
      </li>
      <li>
        <%= link_to "#", class: "mox-moda-navbar-link", "data-balloon" =>"Messages", "data-balloon-pos" => "down" do %>
        <i class="fa fa-comments" aria-hidden="false"></i>
        <% end %>
      </li>
      <li>
        <%= link_to "#", class: "mox-moda-navbar-link", "data-balloon" =>"Notifs", "data-balloon-pos" => "down" do %>
